{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "lib": [
      "ESNext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "web",
    "node_modules",
    "dist",
    ".wrangler"
  ]
}
